{
    "version":2,
    "builds":[
        {
            "src":"/todo-backend/src/main.ts",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/todo-backend/src/main.ts",
            "methods":[
                "GET",
                "POST",
                "UPDATE",
                "DELETE"
            ]
        }
    ]
}